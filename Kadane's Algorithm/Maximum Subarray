class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int n=nums.size();
        int maxsum=-9999;
        int sum=0;
        for(int i=0;i<n;i++)
        {
            sum=0;
            for(int j=i;j<n;j++)
            {
                sum +=nums[j];
                if(sum>maxsum)
                maxsum=sum;
            }
            
        }
        return maxsum;
    }
};
